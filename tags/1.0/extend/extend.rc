    LISTBOX         IDC_LIST_MAILBOX, 16, 15, 136, 55, WS_GROUP | WS_TABSTOP | WS_VSCROLL | LBS_NOINTEGRALHEIGHT | LBS_NOTIFY
    PUSHBUTTON      "&New", IDC_BUTTON_NEW, 158, 15, 50, 14, WS_GROUP | BS_PUSHBUTTON
    PUSHBUTTON      "&Edit", IDC_BUTTON_EDIT, 158, 31, 50, 14, BS_PUSHBUTTON
    PUSHBUTTON      "&Delete", IDC_BUTTON_DELETE, 158, 47, 50, 14, BS_PUSHBUTTON
    GROUPBOX        "General", IDC_STATIC, 7, 83, 211, 94, WS_GROUP
