    EDITTEXT        IDC_EDIT_SUMMARY, 113, 111, 39, 12, ES_AUTOHSCROLL | ES_NUMBER
    CONTROL         "", IDC_SPIN_SUMMARY, UPDOWN_CLASS, UDS_ALIGNRIGHT | UDS_ARROWKEYS | UDS_NOTHOUSANDS | UDS_AUTOBUDDY | UDS_SETBUDDYINT, 139, 111, 13, 12
    LTEXT           "seconds", IDC_STATIC, 158, 111, 34, 12, SS_LEFT | SS_CENTERIMAGE
    AUTOCHECKBOX    "Show summary when get mail", IDC_CHECKBOX_SUMMARY, 38, 125, 114, 11, BS_AUTOCHECKBOX
    LTEXT           "First fetch time on startup:", IDC_STATIC, 16, 141, 92, 12, SS_LEFT | SS_CENTERIMAGE
    EDITTEXT        IDC_EDIT_STARTUP, 113, 141, 39, 12, ES_AUTOHSCROLL | ES_NUMBER
    CONTROL         "", IDC_SPIN_STARTUP, UPDOWN_CLASS, UDS_ALIGNRIGHT | UDS_ARROWKEYS | UDS_NOTHOUSANDS | UDS_AUTOBUDDY | UDS_SETBUDDYINT, 139, 141, 13, 12
    LTEXT           "seconds", IDC_STATIC, 158, 141, 34, 12, SS_LEFT | SS_CENTERIMAGE
    AUTOCHECKBOX    "Icon size:", IDC_CHECKBOX_ICON, 16, 158, 92, 12, WS_GROUP | BS_AUTOCHECKBOX
    EDITTEXT        IDC_EDIT_ICON, 113, 158, 39, 12, ES_AUTOHSCROLL | ES_NUMBER
    CONTROL         "", IDC_SPIN_ICON, UPDOWN_CLASS, UDS_ALIGNRIGHT | UDS_ARROWKEYS | UDS_NOTHOUSANDS | UDS_AUTOBUDDY | UDS_SETBUDDYINT, 139, 158, 13, 12
    LTEXT           "pixels", IDC_STATIC, 157, 158, 34, 12, SS_LEFT | SS_CENTERIMAGE
    DEFPUSHBUTTON   "OK", IDOK, 113, 185, 50, 14, WS_GROUP | BS_DEFPUSHBUTTON
    PUSHBUTTON      "Cancel", IDCANCEL, 168, 185, 50, 14, BS_PUSHBUTTON
END

IDD_URI DIALOG 0, 0, 225, 130
STYLE DS_3DLOOK | DS_CENTER | DS_MODALFRAME | DS_SHELLFONT | WS_VISIBLE | WS_BORDER | WS_CAPTION | WS_DLGFRAME | WS_POPUP | WS_SYSMENU
CAPTION "Server settings"
FONT 8, "Ms Shell Dlg"
BEGIN
    LTEXT           "Protocol:", IDC_STATIC, 7, 7, 50, 12, SS_LEFT | SS_CENTERIMAGE
    COMBOBOX        IDC_COMBO_SCHEME, 59, 7, 53, 26, WS_TABSTOP | WS_TABSTOP | CBS_DROPDOWNLIST | CBS_HASSTRINGS
    LTEXT           "User name:", IDC_STATIC, 7, 25, 50, 12, SS_LEFT | SS_CENTERIMAGE
    EDITTEXT        IDC_EDIT_USER, 59, 25, 83, 12, ES_AUTOHSCROLL
    LTEXT           "Address:", IDC_STATIC, 7, 42, 50, 12, SS_LEFT | SS_CENTERIMAGE
    EDITTEXT        IDC_EDIT_ADDRESS, 59, 42, 159, 12, ES_AUTOHSCROLL
    LTEXT           "Port number:", IDC_STATIC, 7, 59, 50, 12, SS_LEFT | SS_CENTERIMAGE
